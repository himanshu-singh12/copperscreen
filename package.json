{
  "name": "copper-screen-website",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise Digital Consultancy Website - Data-backed growth, Empathy-led strategy",
  "author": "Copper Screen",
  "license": "MIT",
  "homepage": "https://copperscreen.pages.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/himanshu-singh12/copperscreen.git"
  },
  "keywords": [
    "digital-consultancy",
    "seo",
    "web-development",
    "ai-agents",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true next build",
    "preview": "next build && next start",
    "clean": "rmdir /s /q .next out 2>nul || echo \"Clean completed\"",
    "deploy:production": "npm run lint && npm run type-check && npm run build",
    "postbuild": "echo 'Build completed successfully! Ready for Cloudflare Pages deployment.'"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@types/gsap": "^1.20.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "gsap": "^3.14.2",
    "lucide-react": "^0.294.0",
    "next": "^15.1.0",
    "next-seo": "^6.4.0",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-intersection-observer": "^9.5.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
